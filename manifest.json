{
    "name": "chrome-filter",
    "version": "2019.12.1919",
    "manifest_version": 2,
    "description": "chrome-filter",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "popup.js"
            ],
            // 代码注入的时间，可选值： "document_start", "document_end", or "document_idle"，最后一个表示页面空闲时，默认document_idle
            "run_at": "document_idle"
        }
    ]

    
  }
  